{
  "name": "savanna-marketplace",
  "private": true,
  "version": "2.4.1",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "nodemon --exec tsx src/server/index.ts",
    "build": "tsc && vite build && npm run build:backend",
    "build:backend": "tsc --project tsconfig.server.json",
    "start": "node dist/server/index.js",
    "start:dev": "tsx src/server/index.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "type-check:server": "tsc --project tsconfig.server.json --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "db:migrate": "tsx src/server/scripts/migrate.ts",
    "db:seed": "tsx src/server/scripts/seed.ts",
    "db:reset": "npm run db:migrate && npm run db:seed",
    "docker:build": "docker build -t savanna-marketplace .",
    "docker:run": "docker run -p 8080:8080 savanna-marketplace",
    "docker:dev": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "k8s:deploy": "kubectl apply -f k8s/",
    "k8s:delete": "kubectl delete -f k8s/",
    "terraform:init": "cd infrastructure && terraform init",
    "terraform:plan": "cd infrastructure && terraform plan",
    "terraform:apply": "cd infrastructure && terraform apply",
    "terraform:destroy": "cd infrastructure && terraform destroy",
    "monitor:prometheus": "docker-compose -f monitoring/docker-compose.yml up -d",
    "monitor:grafana": "open http://localhost:3000",
    "security:scan": "npm audit && snyk test",
    "security:fix": "npm audit fix && snyk wizard",
    "health:check": "./scripts/health-check.sh",
    "perf:lighthouse": "lighthouse --chrome-flags=\"--headless\" http://localhost:5173",
    "perf:load": "k6 run scripts/load-test.js",
    "backup:db": "pg_dump $DATABASE_URL > backups/$(date +%Y%m%d_%H%M%S).sql",
    "deploy:staging": "echo 'ü¶ì Deploying to staging...' && npm run build && ./scripts/deploy-staging.sh",
    "deploy:production": "echo 'ü¶Å Deploying to production...' && npm run build && ./scripts/deploy-production.sh",
    "setup:dev": "npm install && npm run docker:dev && echo 'ü¶Å Development environment ready!'",
    "cleanup": "rm -rf dist node_modules coverage test-results playwright-report",
    "start:monitoring": "docker-compose -f monitoring/docker-compose.yml up -d prometheus grafana alertmanager",
    "logs": "docker-compose logs -f"
  },
